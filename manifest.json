{
  "manifest_version": 3,
  "name": "Ansioso",
  "description": "Ansioso Extension",
  "version": "1.0",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
        "matches": [
            "<all_urls>"
        ],        
        "js": [
            "jquery.min.js"
        ],
        "run_at": "document_end"
    }
  ]

}